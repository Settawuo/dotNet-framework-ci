<script type="text/javascript">
    function menuResponsiveClick() {
        var x = document.getElementById("menuMain");
        if (x.className === "user_tool_box_set") {
            x.className += " responsive";
            $('#lblMenuIcon').removeClass("glyphicon-list");
            $('#lblMenuIcon').addClass("glyphicon-chevron-down");
        } else {
            x.className = "user_tool_box_set";
            $('#lblMenuIcon').removeClass("glyphicon-chevron-down");
            $('#lblMenuIcon').addClass("glyphicon-list");
        }
    }

    $('.user_tool_box.pay').click(function () {
        $(this).addClass('active').siblings().removeClass('active');

    });

    $(document).ready(function () {
        ais_interface_setup('mini-menu', 'postpaid', 'th', '', false);
    });

    function Reload() {
        window.location.href = window.location.href;
    }

    function ClosePopupReload() {
        if (success == 'True') {
            Reload();
        }
    }

    $(function () {
        $.post('/WBB/GetCurrentCulture').done(function (x) {
            $("#LanguagePage").val(x);
        });

        $("#frmPayment").attr("action", "@ViewBag.SetAction");
    });

    //============================================== EqualHeight ================================
    $(document).ready(function() {
        $('.equal-item').matchHeight({
            byRow: true,
            property: 'min-height'
        });
    });

    $(window).resize(function () {

        var x = document.getElementById("menuMain");
        if (x.className === "user_tool_box_set responsive") {
            x.className = "user_tool_box_set";
            $('#lblMenuIcon').removeClass("glyphicon-chevron-down");
            $('#lblMenuIcon').addClass("glyphicon-list");
        }

        var medsize = window.matchMedia("(min-width: 720px)");
        if (!medsize.matches) {
            $('#divLogo').attr('style', 'min-height: 10px;');
            $('#divHome').attr('style', 'min-height: 10px;');
            $('#divPay').attr('style', 'min-height: 10px;');
            $('#divReportproblem').attr('style', 'min-height: 10px;');
            $('#divLogout').attr('style', 'min-height: 10px;');
            $('#liLogo').attr('style', 'min-height: 10px;');
            $('#liHome').attr('style', 'min-height: 10px;');
            $('#liPayment').attr('style', 'min-height: 10px;');
            $('#liProblem').attr('style', 'min-height: 10px;');
            $('#liLogout').attr('style', 'min-height: 10px;');
        }
    });


    

</script>

