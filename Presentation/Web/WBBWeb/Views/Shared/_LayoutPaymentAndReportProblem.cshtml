@using WBBEntity.PanelModels

@{
    var menuFirstPage = "";
    var menuFirstPageUrl = "";
    var menuPayment = "";
    var menuReportProblem = "";
    var menuLogout = "";
    var menuLogoutController = "";

    if (ViewBag.LogoutController != null)
    {
        menuLogoutController = ViewBag.LogoutController;
    }
    if (ViewBag.LabelFBBOR_MENU != null)
    {
        var lovlist = (List<LovScreenValueModel>)ViewBag.LabelFBBOR_MENU;

        var lovScreenValueModel = lovlist.FirstOrDefault(item => item.Name == "L_MENU_FIRST_PAGE") ?? new LovScreenValueModel();
        menuFirstPage = lovScreenValueModel.DisplayValue;

        lovScreenValueModel = lovlist.FirstOrDefault(item => item.Name == "L_MENU_FIRST_PAGE_URL") ?? new LovScreenValueModel();
        menuFirstPageUrl = lovScreenValueModel.DisplayValue;

        lovScreenValueModel = lovlist.FirstOrDefault(item => item.Name == "L_MENU_PAYMENT") ?? new LovScreenValueModel();
        menuPayment = lovScreenValueModel.DisplayValue;

        lovScreenValueModel = lovlist.FirstOrDefault(item => item.Name == "L_MENU_REPORT_PROBLEM") ?? new LovScreenValueModel();
        menuReportProblem = lovScreenValueModel.DisplayValue;

        lovScreenValueModel = lovlist.FirstOrDefault(item => item.Name == "L_MENU_LOG_OUT") ?? new LovScreenValueModel();
        menuLogout = lovScreenValueModel.DisplayValue;

    }

    Layout = null; 
}


