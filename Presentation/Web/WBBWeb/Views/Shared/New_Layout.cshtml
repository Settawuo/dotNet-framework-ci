<script type="text/javascript">
    @Html.JsMinify(
    @<text>
    var language = "";
    var oldIE;

    $(function () {

        ais_interface_process('mini-menu', 'postpaid', 'th', '', false);

        changeLanguage();

        if ($("#LanguagePage").val() == 1)
            language = "th";
        else
            language = "en";

        $('.responsive').slick({
            dots: false,
            infinite: false,
            speed: 300,
            slidesToShow: 5,
            slidesToScroll: 1,

            responsive: [
              {
                  breakpoint: 1024,
                  settings: {
                      slidesToShow: 5,
                      slidesToScroll: 1
                  }
              },
              {
                  breakpoint: 720,
                  settings: {
                      slidesToShow: 3,
                      slidesToScroll: 2,
                      dots: false,
                      infinite: true

                  }
              }
            ]
        });

        $('#page1').bind('show', function () {
            changePageHeader(1);
        });

        $('#page2').bind('show', function () {
            changePageHeader(2);
        });

        //R18.1 FTTB Sell Router
        $('#pageSelectRouter').bind('show', function () {
            changePageHeader(2);
        });

        $('#page2_1').bind('show', function () {
            if ("@ViewBag.Vas" == "2" || "@ViewBag.TopUp" == "1") {
                changePageHeader_Vas(2);
            } else {
                changePageHeader(3);
            }
        });

        $('#page2_2').bind('show', function () {
            if ("@ViewBag.Vas" == "2" || "@ViewBag.TopUp" == "1") {
                changePageHeader_Vas(2);
            } else {
                changePageHeader(3);
            }
        });

        $('#PackageDetailPage').bind('show', function () {
            changePageHeader(3);
        });

        $('#page3').bind('show', function () {
            changePageHeader(4);
        });

        $('#page4').bind('show', function () {
            if ("@ViewBag.Vas" == "2" || "@ViewBag.TopUp" == "1" || "@ViewBag.Vas" == "4") {
                changePageHeader_Vas(3);
            } else {
                if($("#page4").is(":visible"))
                    changePageHeader(5);
            }
        });

        $('#SelectOntopPage').bind('show', function () {
            changePageHeader_Vas(2);
        });

        $('#OntopDetailPage').bind('show', function () {
            changePageHeader_Vas(2);
        });

        // Detecting IE
        if ($('html').is('.lt-ie7, .lt-ie8, .lt-ie9')) {
            oldIE = true;
        }

        if (oldIE) {
            if (sessionStorage.getItem('detectedIE') != 'detected') {
