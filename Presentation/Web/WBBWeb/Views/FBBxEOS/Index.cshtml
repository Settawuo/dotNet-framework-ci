            }

        .subheader-en, .subheader-th {
            font-size: 34px;
            font-family: db_helvethaicaais_x55_regular;
            background: url(data:image/png;base64,) right center no-repeat;
            max-width: 405px;
            margin-top: 30px;
        }

        .subheader-th {
            color: #000;
        }

            .subheader-th span {
                color: #8dc63f;
            }

        .subheader-en {
