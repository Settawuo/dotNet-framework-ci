                    $.ajax({
                        url: '/Process/getErrorCodeOTP',
                        data: { Error_Code: data3.code, Mobile_No: "SESSION" },
                        dataType: "text",
                        type: 'POST',
                        async: false,
                        success: function (data6) {
                            if (data6 != null && data6 != "") {
                                $('#col-form').hide();
                                $('#col-OTP').hide();
