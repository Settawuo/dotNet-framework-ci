@using WBBEntity.PanelModels
<!DOCTYPE html>

@{
    Layout = null;
    string bOk = "";
    string lLoginFailed = "";
    string lNewCustNotFound = "";
    if (ViewBag.LabelFBBTR001 != null)
    {
        var labelFbbtr001 = (List<LovScreenValueModel>)ViewBag.LabelFBBTR001;
        var lovOk = labelFbbtr001.FirstOrDefault(c => c.Name == "B_OK");
        if (lovOk != null)
        {
            bOk = labelFbbtr001.Any(c => c.Name == "B_OK") ? lovOk.DisplayValue : "";
        }
        var lovLoginFailed = labelFbbtr001.FirstOrDefault(c => c.Name == "L_LOGIN_FAILED");
        if (lovLoginFailed != null)
        {
            lLoginFailed = labelFbbtr001.Any(c => c.Name == "L_LOGIN_FAILED") ? lovLoginFailed.DisplayValue : "";
        }
        var lovNewCustNotFound = labelFbbtr001.FirstOrDefault(c => c.Name == "L_NEW_CUST_NOT_FOUND");
        if (lovNewCustNotFound != null)
        {
            lNewCustNotFound = labelFbbtr001.Any(c => c.Name == "L_NEW_CUST_NOT_FOUND") ? lovNewCustNotFound.DisplayValue : "";
        }
    }
}

