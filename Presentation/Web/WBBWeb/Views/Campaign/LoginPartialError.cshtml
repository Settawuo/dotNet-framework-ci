</script>​

<div id="divLoginError" class="row_ais hidden">
    <div class="col-sm-6 col-md-6" id="col-err">
        <h1 style="color: #91908e">@lLoginFailed</h1>
        <h3 id="lb-failed" style="line-height:160%">@lNewCustNotFound</h3>
        <input id="btnError" class="btn2 btn btn-for-xs" type="button" style="font-size: 20px; border: 0 !important; border-radius: 0 !important;" value="@bOk" />
        <p class="clearfix"></p>
        <p class="clearfix"></p>
    </div>
</div>
