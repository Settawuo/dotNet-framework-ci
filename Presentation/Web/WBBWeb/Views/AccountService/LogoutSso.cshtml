@{
    ViewBag.Title = "Logout";
   
}

<script>
    $(function () {
        $(".wrapSwitchLanguage").hide();
        
        if ("@ViewBag.SessionTimeOut" == "True") {
            alert("Session TimeOut !");
            window.open('', '_self');
            window.close();
        }
        else {
            window.open('', '_self');
            window.close();
        }
       
    });
</script>