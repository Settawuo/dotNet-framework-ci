        W_SERVICE_PAY_2 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_PAY_2").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_PAY_3 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_PAY_3").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_PAY_4 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_PAY_4").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_PAY_5 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_PAY_5").Select(d => d.DisplayValue).FirstOrDefault();

        W_SERVICE_ERECEIPT_1 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_ERECEIPT_1").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_ERECEIPT_2 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_ERECEIPT_2").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_ERECEIPT_3 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_ERECEIPT_3").Select(d => d.DisplayValue).FirstOrDefault();

        W_SERVICE_STORE_1 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_STORE_1").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_STORE_2 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_STORE_2").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_STORE_3 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_STORE_3").Select(d => d.DisplayValue).FirstOrDefault();

        W_SERVICE_7 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_7").Select(d => d.DisplayValue).FirstOrDefault();
