    string URL_DIRECT_PAY_APP = string.Empty;
    string URL_DIRECT_ERECEIPT_APP = string.Empty;
    string URL_DIRECT_EBILL_BROWSER = string.Empty;
    string URL_DIRECT_PAY_BROWSER = string.Empty;
    string URL_DIRECT_ERECEIPT_BROWSER = string.Empty;
    string URL_DIRECT_STORE = string.Empty;

    string URL_DIRECT_EBILL = string.Empty;
    string URL_DIRECT_PAY = string.Empty;
    string URL_DIRECT_ERECEIPT = string.Empty;

    if (ViewBag.FbbConstant != null)
    {
        var FbbConstant = (List<FbbConstantModel>)ViewBag.FbbConstant;
        
        URL_DIRECT_EBILL_APP = FbbConstant.Where(d => d.Field == "URL_DIRECT_EBILL_APP").Select(d => d.Validation).FirstOrDefault();
        URL_DIRECT_PAY_APP = FbbConstant.Where(d => d.Field == "URL_DIRECT_PAY_APP").Select(d => d.Validation).FirstOrDefault();
        URL_DIRECT_ERECEIPT_APP = FbbConstant.Where(d => d.Field == "URL_DIRECT_ERECEIPT_APP").Select(d => d.Validation).FirstOrDefault();
        URL_DIRECT_EBILL_BROWSER = FbbConstant.Where(d => d.Field == "URL_DIRECT_EBILL_BROWSER").Select(d => d.Validation).FirstOrDefault();
