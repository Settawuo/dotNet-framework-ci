        W_SERVICE_6 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_6").Select(d => d.DisplayValue).FirstOrDefault();

        W_SERVICE_EBILL_1 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_EBILL_1").Select(d => d.DisplayValue).FirstOrDefault();
        W_SERVICE_EBILL_2 = LabelFBBOR051.Where(d => d.Name == "W_SERVICE_EBILL_2").Select(d => d.DisplayValue).FirstOrDefault();
