<script type="text/javascript">
    
    function initScriptService() {
        var swiper_thum;
        swiper_thumb = new Swiper('.swiper-thumb', {
            slidesPerView: 1.15, spaceBetween: 15, slidesPerGroup: 1, centeredSlides: false, init: true,
            pagination: { el: '.swiper-thumb .swiper-pagination', clickable: true },
            navigation: { nextEl: '.swiper-thumb .swiper-button-next', prevEl: '.swiper-thumb .swiper-button-prev' },
            breakpoints: {
                480: { slidesPerView: 1.2, centeredSlides: false },
                641: { slidesPerView: 1.2, centeredSlides: false },
                920: { slidesPerView: 2.2, centeredSlides: false },
                1200: { slidesPerView: 2.2, centeredSlides: false }
            }
        });
    }

    function startPageServices() {
        initScriptService();
    }

    function checkDeviceIsMobile(urlBrowser, urlApp) {

        if(
            navigator.userAgent.match(/Android/i) ||
            navigator.userAgent.match(/iPhone/i)
        )
        {
            window.open(`${urlApp}`, '_blank');
        }
        else
        {
            window.open(`${urlBrowser}`, '_blank');
        }
    }

</script>
