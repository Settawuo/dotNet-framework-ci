        URL_DIRECT_STORE = FbbConstant.Where(d => d.Field == "URL_DIRECT_STORE").Select(d => d.Validation).FirstOrDefault();

        if (!Request.Browser.IsMobileDevice)
        {
            URL_DIRECT_EBILL = URL_DIRECT_EBILL_BROWSER;
            URL_DIRECT_PAY = URL_DIRECT_PAY_BROWSER;
            URL_DIRECT_ERECEIPT = URL_DIRECT_ERECEIPT_BROWSER;
