        function ReservedTimeSlot() {
            var chkPass = true;
            var i = $("input[name=SelectPackage]:checked").val();

            var datetemp = $('#CustomerRegisterPanelModel_FBSSTimeSlot_AppointmentDate').val();

            var sentdata = {
                ACCESS_MODE: $("#ACCESS_MODE" + i).val(),
                PROD_SPEC_CODE: $("#SERVICE_CODE" + i).val(),
                ADDRESS_ID: $("#ZTEAddressID").val(),
                LOCATION_CODE: $("#L_LOC_CODE").val(),
                SUBDISTRICT: $('#COVERAGE_L_TUMBOL :selected').text(),
                POSTAL_CODE: $("#CoveragePanelModel_Address_L_ZIPCODE :selected").text(),
                ASSIGN_RULE: $("#CustomerRegisterPanelModel_ASSIGN_RULE").val(),
                AISAIRNUMBER: sessionStorage["AisAirNumber"].toString(),
                SUB_ACCESS_MODE: $('#CustomerRegisterPanelModel_SUB_ACCESS_MODE').val(),
                SERVICE_LEVEL: $("#ServiceLevel").val(),
                AREA_REGION: $("#CoveragePanelModel_Address_AreaRegion").val(),
                EVENT_RULE: "@EVENT_RULE",
                SPECIAL_SKILL: ""
            };

            if ($("#PlugAndPlayFlow").val() == "Y") {
                sentdata.APPOINTMENT_DATE = parseInt(datetemp.split('/')[0].toString()) + "-" + datetemp.split('/')[1] + "-" + datetemp.split('/')[2];
                sentdata.TIME_SLOT = $('#CustomerRegisterPanelModel_FBSSTimeSlot_TimeSlot').val();
            }
            else if ($("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSAppointmentDate']").val() != undefined
                    && $("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSTimeSlot']").val() != undefined) {
                sentdata.APPOINTMENT_DATE = $("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSAppointmentDate']").val();
                sentdata.TIME_SLOT = $("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSTimeSlot']").val();
            }

            if ($("#ZTEAddressID").val() != "" && $("#L_EVENT_CODE").val() == "") {
                if ((
                        $("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSAppointmentDate']").val() != undefined
                        && $("#divTimeSlot").find("i.color-green").closest("div").find("input[name='FBSSTimeSlot']").val() != undefined
                        && $('#hdUseTimeslot').val() == "Y"
                    )
                    || $("#PlugAndPlayFlow").val() == "Y") {
                    $.ajax({
                        type: "GET",
                        url: "/CustomerRegister/ReservedTimeslot",
                        data: sentdata,
                        dataType: "json",
                        async: false,
                        success: function (data) {
                            if (data.RESULT == "0") {
                                $("#SummaryPanelModel_RESERVED_ID").val(data.RESERVED_ID);
                                $('#CustomerRegisterPanelModel_FBSSTimeSlot_TimeSlot').val(sentdata.TIME_SLOT);
                                $('#CustomerRegisterPanelModel_FBSSTimeSlot_AppointmentDate').val(datetemp);
                            }
                            else if (data.RESULT == "-2") {
                                $("#SummaryPanelModel_RESERVED_ID").val("");
                                chkPass = false;
                            }
                            else {
                                $("#SummaryPanelModel_RESERVED_ID").val("");
                            }
                        },
                        error: function () {
                            $("#SummaryPanelModel_RESERVED_ID").val("");
                            chkPass = false;
                        }
                    });
                } else {
                    $("#SummaryPanelModel_RESERVED_ID").val("");
                }
            }
            else {
                $("#SummaryPanelModel_RESERVED_ID").val("");
            }

            return chkPass;
        }
