        function validateEmailEbillSubmit(email) {
            var filter = /^(((([a-zA-Z0-9\_\-]{1,50})+(([\.]{1,50}[a-zA-Z0-9\_\-]{1,50}){0,50})){1,95})+@@([\w-]+\.)+[\w-]{2,4})?$/;
            if (email.substr(0, 2) == "-@@" || email.substr(0, 2) == "_@@") {
                return false;
            }
            if (!(email.indexOf('@@') > -1)) {
                return false;
            }
            if (filter.test(email)) {
                return true;
            }
            else {
                return false;
            }
        }


