    string L_SUMMARY = "";
    string L_BAHT = "";
    string L_SUMM_DETAIL11 = "";
    string L_SUMM_DETAIL12 = "";
    string L_SUMMARY_TOPUP_INTERNET = "";

    //11
    string B_CHOOSE_VAS_INTERNET = "";

    //0
    string B_NEXT = "";
    string B_BACK = "";
    string L_CHOOSE_TOPUP_INTERNET = "";

    string MobileFromBypass = "";

