    function containPrivilegeV2(Privilege, Ontop) {
        if (Privilege[0].length > 0) { // Buy free trial
            $.each(Privilege[0], function (i, pr) { //get prvilege
                $.each(Ontop, function (a, on) { //get on top
                    if (on != undefined) {
                        if (pr.productCode == on[0]) {
                            Ontop.splice(a, 1);
                        }
                    }
                });
            });
        }

        if ($("#PruductCD_Content").val() != "") {
            var sffPromoContent = $("#PruductCD_Content").val();
            $.each(Ontop, function (a, on) { //get on top
                if (on != undefined) {
                    if (on[0] == sffPromoContent) {
                        Ontop.splice(a, 1);
                    }
                }
            });
        }
        arrOntop = Ontop;
    }

    function filterWifiLog(Ontop) {
        //R20.7 Wifi Log
        $.each(Ontop, function (a, on) {
            if (on != undefined) {
                if (myPackagePromotionList.length > 0) {
                    $.each(myPackagePromotionList, function (iMyPackagePromotionList, eMyPackagePromotionListData) {
                        if (on[0] == eMyPackagePromotionListData) {
                            if (myPackageTopupInternetNotRepeat.length > 0) {
                                $.each(myPackageTopupInternetNotRepeat, function (iMyPackageTopupInternetNotRepeat, eMyPackageTopupInternetNotRepeatData) {
                                    if (on[0] == eMyPackageTopupInternetNotRepeatData) {
                                        Ontop.splice(a, 1);
                                    }
                                });
                            }
                        }
                    });
                }
            }
        });
        arrOntop = Ontop;
    }

