    {
        var LabelFBBORV24 = (List<LovScreenValueModel>)ViewBag.LabelFBBORV24;
        L_SELECT_VAS_ONTOP_INTERNET = LabelFBBORV24.Any(c => c.Name == "L_SELECT_VAS_ONTOP_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_SELECT_VAS_ONTOP_INTERNET").DisplayValue : "";
        L_PLEASE_SELECT_VAS_INTERNET = LabelFBBORV24.Any(c => c.Name == "L_PLEASE_SELECT_VAS_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_PLEASE_SELECT_VAS_INTERNET").DisplayValue : "";
