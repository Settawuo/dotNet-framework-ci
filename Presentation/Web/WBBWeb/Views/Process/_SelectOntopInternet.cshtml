        L_MESSAGE_NOT_REGISTER_INTERNET = LabelFBBORV24.Any(c => c.Name == "L_MESSAGE_NOT_REGISTER_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_MESSAGE_NOT_REGISTER_INTERNET").DisplayValue : "";

