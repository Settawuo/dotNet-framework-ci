                        tmpPackageContent.sort((a, b) => a.PRICE_CHARGE - b.PRICE_CHARGE);//R2023.09 28Sep2023 Max Kunlp885 E-welcome
                        arrOntop.sort((a, b) => tmpPackageContent.findIndex(item => item.SFF_PROMOTION_CODE === a[0]) - tmpPackageContent.findIndex(item => item.SFF_PROMOTION_CODE === b[0]));

                        let promocodePriceFree = "";
                        const hasPriceFree = tmpPackageContent.some(package => {
                            const matchingDetail = tmpContentDetail.find(detail => detail.DataName === package.SFF_PROMOTION_CODE);
                            if (package.PRICE_CHARGE === 0 && matchingDetail) {
                                //R2023.11 23Nov2023 Max Kunlp885 E-welcome
                                promocodePriceFree = package.SFF_PROMOTION_CODE;
                                return true;
                            }
                            return false;
                        });


                        $.each(data.DataGroup, function (arrDataGroupIndex, arrDataGroup) {
                            var setGroupName = arrDataGroup;
                            var indexdataingroup = 1;
                            $.each(arrOntop, function (arrOntopIndex, arrOntopData) {
                                var lovDataContent = tmpContentDetail.filter(function (elem) {
                                    return (elem.DataName == arrOntopData[0]
                                        && elem.GroupName == arrDataGroup);
                                });

                                if (lovDataContent != undefined && lovDataContent.length > 0) {
                                    strBuilder += setHtmlContent(indexData, lovDataContent[0], setGroupName, indexdataingroup);
                                    setGroupName = "";
                                    indexdataingroup++;
                                    indexData++
                                }
                            });
                        });

