        L_SUMM_DETAIL11 = LabelFBBORV24.Any(c => c.Name == "L_SUMM_DETAIL11_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_SUMM_DETAIL11_INTERNET").DisplayValue : "";
        L_SUMM_DETAIL12 = LabelFBBORV24.Any(c => c.Name == "L_SUMM_DETAIL12_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_SUMM_DETAIL12_INTERNET").DisplayValue : "";
        L_SUMMARY_TOPUP_INTERNET = LabelFBBORV24.Any(c => c.Name == "L_SUMMARY_TOPUP_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_SUMMARY_TOPUP_INTERNET").DisplayValue : "";

        B_CHOOSE_VAS_INTERNET = LabelFBBORV24.Any(c => c.Name == "B_CHOOSE_VAS_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "B_CHOOSE_VAS_INTERNET").DisplayValue : "";

        B_NEXT = LabelFBBORV24.Any(c => c.Name == "B_NEXT_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "B_NEXT_INTERNET").DisplayValue : "";
        B_BACK = LabelFBBORV24.Any(c => c.Name == "B_BACK_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "B_BACK_INTERNET").DisplayValue : "";
        L_CHOOSE_TOPUP_INTERNET = LabelFBBORV24.Any(c => c.Name == "L_CHOOSE_TOPUP_INTERNET") ? LabelFBBORV24.FirstOrDefault(c => c.Name == "L_CHOOSE_TOPUP_INTERNET").DisplayValue : "";
    }

    //R20.7 Wifi Log
    string[] TOPUP_INTERNET_NOT_REPEAT;
    if (ViewBag.FbbConstant != null)
    {
        var FbbConstant = (List<FbbConstantModel>)ViewBag.FbbConstant;
        TOPUP_INTERNET_NOT_REPEAT = FbbConstant.Where(p => p.Field == "TOPUP_INTERNET_NOT_REPEAT").Select(p => p.Validation).ToArray();
    }
    else
    {
        TOPUP_INTERNET_NOT_REPEAT = null;
    }

    if (null != ViewBag.ExistingFlag)
