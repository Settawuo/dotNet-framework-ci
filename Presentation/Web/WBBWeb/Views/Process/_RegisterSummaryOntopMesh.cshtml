    //R22.11 Mesh with arpu
    function onClickChkPointArpu(divId,lovMeshPoint) {
        var thisVasHasChecked = $("#" + divId.id).is(":checked");

        var point = parseFloat($("#MESH_ARPU_POINTS").val());
        var numPointSelect = parseFloat($("#MESH_ARPU_POINTS_SELECT").val());

        if (thisVasHasChecked) {
            $("#" + divId.id).prop("checked", true);
            $("#lbl_" + divId.id).css("font-weight", "700");
            point = point + lovMeshPoint;
            numPointSelect = numPointSelect + 1;
        }
        else {
            $("#" + divId.id).prop("checked", false);
            $("#lbl_" + divId.id).css("font-weight", "100");
            point = point - lovMeshPoint;
            numPointSelect = numPointSelect - 1;
        }

        $("#MESH_ARPU_POINTS").val(point);
        $("#MESH_ARPU_POINTS_SELECT").val(numPointSelect);
        }

