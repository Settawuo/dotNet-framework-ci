        function CheckContactPhone(callback) {
            var contactPhoneValue = $("#CustomerRegisterPanelModel_L_CONTACT_PHONE").val();

            if (contactPhoneValue) {
                callback();
            }
            else {
                $.ajax({
                    url: '/Process/GetOtherInformation',
                    data: {
                        mobileno: $('#CoveragePanelModel_P_MOBILE').val(),
                        caid: $('#CoveragePanelModel_CA_ID').val(),
                        baid: $('#CoveragePanelModel_BA_ID').val(),
                        selectvalue: 'CONTACT'
                    },
                    dataType: "json",
                    type: 'POST',
                    success: function (response1) {
                        if (response1 !== "") {
                            var arrayRespon = response1.split(',');
                            $('#CustomerRegisterPanelModel_L_CONTACT_PHONE').val(arrayRespon[2]);
                        }
                        if (typeof callback === 'function') {
                            callback();
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error(xhr, status, error);
                    }
                });
            }
        }

