                             // point >= 300
                             chkShowSummarytab3 = true;

                             summarytab3Detail += "<tbody>";

                             //หมายเลข {x} มีจำนวน AIS Point {y} Point
                             var nonMobile = $("#CoveragePanelModel_P_MOBILE").val();
                             var strSUMMARY_DISPLAY_POINT_1 = "@SUMMARY_DISPLAY_POINT_1";
                             strSUMMARY_DISPLAY_POINT_1 = strSUMMARY_DISPLAY_POINT_1.replace("{x}", privacyMsisdn(nonMobile));
                             strSUMMARY_DISPLAY_POINT_1 = strSUMMARY_DISPLAY_POINT_1.replace("{y}", "<font color='red'>" + InternetPoints + "</font>");

                             summarytab3Detail += "<tr><th><span style='font-weight: bold'>" + strSUMMARY_DISPLAY_POINT_1 + "</span></th></tr>";

                             //**ใช้ AIS Point {x} Points แลกรับส่วนลดค่าบริการ Mesh WiFiมูลค่า 50 บาทนาน 12 รอบบิลต่อ 1 จุด
                             var strSUMMARY_DISPLAY_POINT_2 = "@SUMMARY_DISPLAY_POINT_2";
                             strSUMMARY_DISPLAY_POINT_2 = strSUMMARY_DISPLAY_POINT_2.replace("{x}", lovMeshPointDiscount);

                             summarytab3Detail += "<tr><td><span>" + strSUMMARY_DISPLAY_POINT_2 + "</span></td></tr>";

                             //Point ที่มี สามารถโชว์ได้กี่จุด
                             var numShowChkPoint = Math.floor(InternetPoints / lovMeshPointDiscount);

                             var runNumber = 0;
                             var strSUMMARY_DISPLAY_POINT_3 = "";
                             while (runNumber < numberMesh) {
                                 if ((runNumber + 1) <= numShowChkPoint) {

                                     strSUMMARY_DISPLAY_POINT_3 = "@SUMMARY_DISPLAY_POINT_3";//แลก Mesh จุดที่ {x} ด้วย  Point AIS Fibre
                                     strSUMMARY_DISPLAY_POINT_3 = strSUMMARY_DISPLAY_POINT_3.replace("{x}", (runNumber + 1).toString());

                                     summarytab3Detail += "<tr><td><span><label id='lbl_chkPointArpu" + (runNumber + 1) + "' style='font-weight: 100;'>";
                                     summarytab3Detail += "<input type='checkbox' id='chkPointArpu" + (runNumber + 1) + "' name='chkPointArpu' onclick='onClickChkPointArpu(chkPointArpu" + (runNumber + 1) + "," + lovMeshPointDiscount +")'>";
                                     summarytab3Detail += " " + strSUMMARY_DISPLAY_POINT_3 + " ";
                                     summarytab3Detail += "</label></span></td></tr>";
                                 }
                                 runNumber++;
                             }

                             summarytab3Detail += "</tbody>";
                             $('#tblMeshSummaryWithArpu').html(summarytab3Detail);
                             $('#summarytab3').show();
                         }
                     }
                     else {
                         chkShowSummarytab3 = false;
                         $('#summarytab3').hide();
                     }

                }
            });
        });

    }

