    function onSubmit() {
        LockScreen(true, "");

        if ("@vas_flag" != "0")
            $("[name*='SummaryPanelModel.VAS_FLAG']").val("@vas_flag");

        $.ajax({
            url: '/Process/ValidateCaptcha',
            data: { inputUser: $('#defaultReal').val(), inputHash: $('#defaultReal').realperson('getHash') },
            dataType: "json",
            async: false,
            type: 'POST',
            success: function (response) {
                if (!response) {
                    toastrErrorMessage("@L_CAPTCHA_ERR");
                    $("#defaultReal").removeClass('form-control').addClass('form-control errorinput');
                    LockScreen(false);
                }
                else {
                    $("#SummaryPanelModel_L_SEND_EMAIL").val(true);
                    var tempmail = $('#CustomerRegisterPanelModel_L_EMAIL').val()
                    if (tempmail == undefined || tempmail == "") {
                        $('#CustomerRegisterPanelModel_L_EMAIL').val($("#NEW_L_EMAIL").val());
                    }

                    // 18.4  Add Parameter Existing
                    var locationCd = "";
                    var ascCode = "";
                    var employee_id = "";
                    // Line officer : Send => Location Code , ASC Code , Employee ID
                    // Line other : Send => Location Code = "" , ASC Code = "" , Employee ID = ""
                    if (window.parent.$("#isOfficer").length > 0 && window.parent.$("#isOfficer").val() == "Y") {
                        if (window.parent.$("#LcCode").length > 0) {
                            locationCd = window.parent.$('#LcCode').val();
                            $("#Location_Code").val(locationCd);
                        }

                        if (window.parent.$("#ASCCode").length > 0) {
                            ascCode = window.parent.$('#ASCCode').val();
                            $("#ASC_Code").val(ascCode);
                        }

                        if (window.parent.$("#EmployeeID").length > 0) {
                            employee_id = window.parent.$('#EmployeeID').val();
                            $("#Employee_ID").val(employee_id);
                        }
                    }
                    else {
                        $("#Location_Code").val("");
                        $("#ASC_Code").val("");
                        $("#Employee_ID").val("");
                    }
                    //-----

                    $("#btnSubmit").hide();
                    $("#btnNoAccept").show();
                    $("#iForm").submit(); //save
                }
            }
        });
    }

