<!DOCTYPE html>
<!--[if lt IE 7]> <html class="lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <title id="txttitle"></title>
    @{
        string th = "";
        string en = "";
        var dataTitle = WBBWeb.Extension.WebExtension.Get_TitleName();
        if (dataTitle.Any())
        {
            var data = dataTitle.FirstOrDefault();
            th = data.LovValue1.ToString();
            en = data.LovValue2.ToString();
        }
    }

    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
    @Styles.Render("~/Content/css/bootsrapcss")
    <link href="~/Content/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" />

    <link href="http://localhost:50960/respond-proxy.html" id="respond-proxy" rel="respond-proxy" />
    <link href="~/Scripts/respond.proxy.gif" id="respond-redirect" rel="respond-redirect" />
    @Styles.Render("~/Content/css/rabbit")

    <style>
        /*footer {
            background-color: #DCDE40;
            height: 25px;
            position: fixed;
            bottom: 0;
            left: 0;
            right: 0;
        }*/

        h1 {
            color: #558C0B;
            font-size: 5em;
            margin-bottom: 0;
        }

        h3 {
            font-size: 2em;
            margin-top: 0;
            margin-bottom: 30px;
        }

        p {
            font-size: 22px;
            font-weight: normal;
            color: #4D4D4D;
            margin-bottom: 0;
        }

        input[type=text] {
            border-radius: 0;
            box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
            font-size: 21px;
        }

        select {
            border-radius: 0 !important;
            padding-top: 2px !important;
            font-size: 21px !important;
        }

            select option {
                font-size: 21px;
            }

        .btn1 {
            padding: 13px 22px;
            background-color: #8dbf65;
            color: #fff;
        }

            .btn1:hover {
                padding: 13px 22px;
                background-color: #8dbf65;
                color: #fff;
                text-decoration: none;
                cursor: not-allowed;
            }

        .btn2 {
            padding: 13px 22px;
            background-color: #c1da0e;
            color: #fff;
        }

            .btn2:hover {
                padding: 13px 22px;
                background-color: #4a831c;
                color: #fff;
                text-decoration: none;
            }

        .btn3 {
            padding: 13px 35px;
            background-color: #ccc;
            color: #fff !important;
        }

            .btn3:hover {
                padding: 13px 35px;
                background-color: #aaa;
                color: #fff !important;
                text-decoration: none;
            }


    </style>
</head>

<body>
    <input type="hidden" id="txtTH" value="@th" />
    <input type="hidden" id="txtEN" value="@en" />
    <nav class="navbar navbar-default" style="box-shadow:0px 0px 100px rgba(0, 0, 0, 0.2);border-bottom:1px solid#ccc;">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" style="padding-top:8px;" href="#">
                    <img src="data:image/png;base64," width="81" height="35" />
                </a>
            </div>
            

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="/WBB/ChangeCurrentCulture?culture=2" id="btnTh">
                            <img src="data:image/png;base64," alt="" border="0" />
                        </a>
                        <a href="/WBB/ChangeCurrentCulture?culture=1" class="i-hidden" id="btnEn">
                            <img src="data:image/png;base64," alt="" border="0" />
                        </a>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
</body>
</html>

@Scripts.Render("~/bundles/js/jquery19")
@Scripts.Render("~/bundles/js/rabbit")
@Scripts.Render("~/bundles/js/boostrapjs")

<script>
    $(function () {
        var x = $("#LanguagePage").val();
        if (x == 1) {
            $('#txttitle').html($('#txtTH').val());
        }
        else {
            $('#txttitle').html($('#txtEN').val());
        }
    });
</script>