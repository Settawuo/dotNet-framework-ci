        var result = false;
        isHavePackage = false;

        $.ajax({
            type: "POST",
            url: "/SelectPackage/GetListServiceAndPromotionByPackType",
            data: {
                "mobileNo": $.trim($("#no").val()),
                "idCard": $.trim($("#IDCardNo").val()),
                "idCardType": $("#IDCardTypeENG").val()
            },
            dataType: "json",
            async: false,
            success: function (dataService) {

                if ($.trim(dataService.errorMessage) != "" || dataService.resultFlag == "N") {
                    isHavePackage = false;
                } else if (dataService.v_owner_product == "") {
                    isHavePackage = false;
                } else {

                    $('#P_SFF_PROMOCODE').val(dataService.v_sff_main_promotionCD);
                    $("#v_package_subtype").val(dataService.v_package_subtype);
                    $("#ZTEAddressID").val(dataService.addressId);

                    //R20.6 Order Relocate change technology mesh : add v_owner_product
                    $("#v_owner_product").val(dataService.v_owner_product);

                    //R21.5
                    var P_SALE_CHANNEL = "CUSTOMER";
                    P_SALE_CHANNEL = !($("#P_SALE_CHANNEL").val() == null || $("#P_SALE_CHANNEL").val() == undefined) ? "OFFICER" : "CUSTOMER";
                    //R21.10
                    sessionStorage.setItem("P_SALE_CHANNEL", P_SALE_CHANNEL);
                    //R22.03 Topup Replace
                    $("#pbox_count").val(dataService.v_number_of_pb_number);

                    $.ajax({
                        type: "GET",
                        url: "/SelectPackage/GetListPackagebySFFPromoV2",
                        data: {
                            "P_OWNER_PRODUCT": dataService.v_owner_product,
                            "P_PRODUCT_SUBTYPE": dataService.v_package_subtype,
                            "accessMode": "",
                            "no": $.trim($("#no").val()),
                            "P_SFF_PROMOCODE": dataService.v_sff_main_promotionCD,
                            "P_EXISTING_REQ": "PBOX",
                            "P_SALE_CHANNEL": P_SALE_CHANNEL //R21.5
                        },
                        dataType: "json",
                        async: false,
                        success: function (dataSffPromo) {

                            if (dataSffPromo.length == 0) {
                                isHavePackage = false;
                            } else {

                                $.ajax({
                                    type: "GET",
                                    url: "/SelectPackage/GetPlayboxExtensionInfo",
                                    data: {
                                        "AisAirNumber": $.trim($("#no").val()),
                                        "UsedNumberOfPlaybox": dataService.v_number_of_pb_number,
                                        "NumberOfPlaybox": dataService.L_NUMBER_OF_PLAYBOX
                                    },
                                    dataType: "json",
                                    async: false,
                                    success: function (dataExt) {

                                        if (dataExt.UsePlayboxList != null && (dataExt.UsePlayboxList.length > 0)) {
                                            isHavePackage = true;
                                            return;
                                        }
                                        //R22.04 Issue Playbox
                                        var countPB = dataService.v_number_of_pb_number != null ? parseInt(dataService.v_number_of_pb_number) : 0;
                                        if (countPB == 0) { // PBox Main
                                            var mainPlayboxCode = dataExt.MainPlayboxCode;
                                            var rowSffPromoIndexMain = 0;
                                            while (dataSffPromo.length > rowSffPromoIndexMain) {
                                                if (dataSffPromo[rowSffPromoIndexMain]["SERVICE_CODE"] == mainPlayboxCode) {
                                                    isHavePackage = true;
                                                    return;
                                                }

                                                if (isHavePackage) return;

                                                rowSffPromoIndexMain++;
                                            }
                                        }
                                        else { // PBox 2,3
                                            if (dataExt.AvailPlayboxList != null && (dataExt.AvailPlayboxList.length > 0) && (dataSffPromo.length > 0)) {
                                                var rowAvailPlayboxIndex = 0;
                                                while (dataExt.AvailPlayboxList.length > rowAvailPlayboxIndex) {

                                                    var productCode = dataExt.AvailPlayboxList[rowAvailPlayboxIndex]["ProductCode"];

                                                    var rowSffPromoIndex = 0;
                                                    while (dataSffPromo.length > rowSffPromoIndex) {

                                                        if (dataSffPromo[rowSffPromoIndex]["SERVICE_CODE"] == productCode) {
                                                            isHavePackage = true;
                                                            return;
                                                        }
                                                        rowSffPromoIndex++;
                                                    }

                                                    if (isHavePackage) return;

                                                    rowAvailPlayboxIndex++;
                                                }
                                            }
                                        }
                                    }
                                });
                            }
                        }
                    });
                }

                if (isHavePackage) {
                    $('#topupplaybox').show();
                    $('#menu-7-responsive').show();
                } else {
                    $("#topupplaybox").hide();
                    $("#menu-7-responsive").hide();
                }
            }
        });

        return isHavePackage;
    }

