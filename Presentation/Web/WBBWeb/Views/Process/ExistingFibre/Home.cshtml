                        iframe_Load();
                        _Result = false;
                    }
                }
            }
            else if (_frameMain_src[2] + "/" + _frameMain_src[3] == "process/topup") {
                if ($('#frameMain').contents().find('#fibre_container').find('#myModal').prop('class') == "modal fade") {
                    if ($('#frameMain').contents().find('#IDCardNo').val() == undefined) {
                        iframe_Load();
                        _Result = false;
                    }
                    else if ($('#frameMain').contents().find('#IDCardNo').val().length == 0) {
                        iframe_Load();
                        _Result = false;
                    }
                }
            }
            else if (_frameMain_src[2] + "/" + _frameMain_src[3] == "process/topupfixedline") {
                if ($('#frameMain').contents().find('#fibre_container').find('#myModal').prop('class') == "modal fade") {
                    if ($('#frameMain').contents().find('#IDCardNo').val() == undefined) {
                        iframe_Load();
                        _Result = false;
                    }
                    else if ($('#frameMain').contents().find('#IDCardNo').val().length == 0) {
                        iframe_Load();
                        _Result = false;
                    }
