    <script type="text/javascript">
    $('#btnNoAccept2').hide();
    var validateModel004R = new Array();

    var registerSummary = new RegisterSummary();
    var checkvas = '@vas_flag'; 

    var outsubTypeShop;

    function RegisterSummary() {

        this.backPage = function (type) {

            if (type == 0) {
                // Update 17.4 Script Adobe Analytic
                digitalData.customLinkName = "summary:cancel-button";
                console.log("custom-link-click : funcn registerSummary.backPage Type 0");
                
                document.getElementById("ChkCon").checked = false;

                if ("@vas_flag" == 2 || "@topUp" == "1") {
                    $('#headvasnf').text("-");
                    $("#page4").hide();
                    $('#page2_1').show();
                }
                else {
                    $("#page4").hide();
                    $("#page3").show();
                }
            }
            else if (type == 1) {
                // Update 17.4 Script Adobe Analytic
                digitalData.customLinkName = "summary:cancel-button";
                console.log("custom-link-click : funcn registerSummary.backPage Type 1");
                

                var selValue = $('input[name=typeselect]:checked').val();
                if ("@vas_flag" == 2 || "@topUp" == "1") selValue = $('#outcardType').val();

                if (selValue == 'R') {
                    $("#summarytab0").show();
                    $("#summarytab1").hide();
                }
                else {
                    $("#summarytab0").hide();
                    $("#summarytab1").show();
                    if ("@vas_flag" == 2 || "@topUp" == "1") {
                        $("#summarytab5").hide();
                        $("#summarytab5Line2").show();
                    }
                    else {
                        $("#summarytab5Line2").hide();
                    }
                }

                $("#summarytab9").show();
                if ("@vas_flag" == 2 || "@topUp" == "1") {
                    $("#summarytab3").hide();
                    $("#summarytab4").hide();
                    $("#summarytab3Line2").show();
                    $("#summarytab4Line2").show();
                }
                else {
                    $("#summarytab3Line2").hide();
                    $("#summarytab4Line2").hide();
                }

                if ($('#HdFunctionSubProduct').val() == 'SWiFi') {
                    $("#summarytab7").show();
                }
                else {
                    $("#summarytab7").hide();
                }
                $("#summarytab6").show();
                $("#summarytab8").show();
                $("#summarytab8_1").show();
                $("#summarytabEstatement").show();
                $("#summarytabCaptcha").show();
                $("#summarytabSignature").hide();
                if (checkPanelPreviewFile()) {
                    $("#previewfile").show();
                }

                $.ajax({
                    type: "GET",
                    url: "/PrePrint/getSubtype",
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        if (data.indexOf($("#CustomerRegisterPanelModel_outSubType").val()) == -1 && $("#TopUp").val() == "5") {
                            $("#conditions1").hide();
                            $("#conditions2").show();

                            $("#btnSubmit").hide();
                            $("#btnNoAccept").show();

                            $("#summarytab3").show();
                            $("#summarytab4").show();
                        }
                        else if (checkvas != "2" && "@topUp" != "1") {
                            $("#conditions1").show();
                            $("#conditions2").hide();


                            $("#summarytab3").hide();
                            $("#summarytab4").hide();
                        }
                        else {
                            $("#conditions1").hide();
                            $("#conditions2").hide();

                            $("#summarytab3").show();
                            $("#summarytab4").show();
                        }
                    }
                });

            }

        }

        this.nextPage = function () {
            

            digitalData.customLinkName = "summary:next-button";
            console.log("custom-link-click : funcn registerSummary.nextPage");
            

            $("#page4").hide();
            $("#preprint").show();
            GeneratePrePrintForm();

            //script Adobe Analytic
            var lang = $("#LanguagePage").val() == 1 ? "th" : "en";
            digitalData.page = {
                pageInfo: {
                    pageName: "newfibre:" + lang + ":PrePrintForm",
                    site: "newfibre",
                    language: lang
                }
            }
            console.log("page load PrePrintForm : ", lang);
            
        }

    }

    function DataBindPage() {
        var pvFile = checkPanelPreviewFile();
        if (pvFile) {
            $("#previewfile").show();
        }

        $('#PDF_Detail2').html("");
        $("#listvas").html("");
        $('#btnNoAccept2').hide();
        if ($('#HdFunctionSubProduct').val() == 'SWiFi') {

            $("#summarytab7").show();
        }
        else {

            $("#summarytab7").hide();
        }

        $('#SummaryPanelModel_PackageModel_SelectVas_Flag').val($('#hdGetVoip').val());
        $('#SummaryPanelModel_PackageModel_SelectPlayBox_Flag').val($('#hdGetPlayBox').val());
        $('#SummaryPanelModel_PackageModel_SelectPlayPBL_Flag').val($('#hdGetBPL').val());
        var indexlist_ontop = 0;
        if ("@vas_flag" != "2" && "@topUp" != "1") {

            var selValue = $('input[name=typeselect]:checked').val();

            if (selValue == 'R') {
                $("#summarytab0").show();
                $("#summarytab1").hide();
                $("#summarytab5").hide();
            }
            else {
                $("#summarytab0").hide();
